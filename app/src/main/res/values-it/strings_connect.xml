    <string name="connector_communication_failed_msg"><![CDATA[Printoid could not communicate with your OctoPrint server from %1$s. Please check your network connection and your configuration then retry.<br><br><b>Extra informations:</b><br><br>%2$s]]></string>

    <!-- CONNECTOR STEP 3 (CHECK API) -->
    <string name="connector_testing_title">Connessione step 3 (API test)</string>
    <string name="connector_testing_msg">Printoid sta comunicando con la tua stampante %1$s&#8230;\n\nIndirizzo: %2$s</string>
