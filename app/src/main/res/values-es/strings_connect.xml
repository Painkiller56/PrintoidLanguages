    <string name="connector_communication_failed_msg"><![CDATA[Printoid no se puede conectar con su servidor OctoPrint desde %1$s. Por favor revise su conexión de red y su configuración después reintente.<br><br><b>Más información:</b><br><br>%2$s]]></string>

    <!-- CONNECTOR STEP 3 (CHECK API) -->
    <string name="connector_testing_title">Conexión paso 3 (Test API)</string>
